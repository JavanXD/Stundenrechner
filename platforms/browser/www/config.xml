<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.stundenrechner" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Stundenrechner</name>
    <description>Arbeitszeiterfassung für die Arbeit. Stundenrechner zählt die Stunden abzüglich der Pause.</description>
    <author email="support@javan.de" href="https://javan.de">Javan Rasokat</author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
